<Application x:Class="MarkRead.App.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:MarkRead.App"
             xmlns:converters="clr-namespace:MarkRead.App.UI.Converters"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <converters:BoolToVisibilityConverter x:Key="BoolToVisibilityConverter"/>
        <converters:NodeTypeToIconConverter x:Key="NodeTypeToIconConverter"/>
        <converters:ZeroToVisibilityConverter x:Key="ZeroToVisibilityConverter"/>
        <converters:BoolToGridLengthConverter x:Key="BoolToGridLengthConverter"/>
    </Application.Resources>
</Application>
